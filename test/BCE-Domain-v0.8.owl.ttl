###  http://ldp.e.corpintra.net/ontology/bce#glossaryParent

:glossaryParent rdf:type owl:ObjectProperty ;
                
                rdfs:label "glossary parent" ;
                
                rdfs:comment """Glossary parent is pointing to another touchpoint that shares the same glossary entry.

Excel comment:
This field is for TPs like Newsletter or MB-Magazine, to group them together to only one Glossary entry. Important: No virtual parents here! Parent MUST appear in the map! And no multilevel-nesting!
""" ;
                
                skos:altLabel "glossaryparent" ;
                
                rdfs:isDefinedBy "http://ldp.e.corpintra.net/ontology/bce#" ;
                
                rdfs:domain :Touchpoint ;
                
                rdfs:range :Touchpoint ;
                
                rdfs:subPropertyOf :bceObjectProperty .




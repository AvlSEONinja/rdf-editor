###  http://ldp.e.corpintra.net/ontology/bce#bceObjectProperty

:bceObjectProperty rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "bce object property" ;
                   
                   rdfs:comment "Top object property for BCE" ;
                   
                   rdfs:isDefinedBy "http://ldp.e.corpintra.net/ontology/bce#" .



###  http://ldp.e.corpintra.net/ontology/bce#channel

:channel rdf:type owl:ObjectProperty ;
         
         rdfs:label "channel" ;
         
         rdfs:isDefinedBy "http://ldp.e.corpintra.net/ontology/bce#" ;
         
         rdfs:comment """Channel attribute of touchpoint

benutzt von Leuten ausserhalb des Projekts
Granulierung 3 x

BCEchannels : Evtl. Problem, da teilw. Überschneidung

grobe Clusterung für Leute innerhalb des Projekts

Granulierung etwas anders/feiner 5 Stück

2 versch. Klassifikationssysteme


Channel/BceChannel ist im Moment auch grob Orga Grenze bei BCE
""" ;
         
         skos:altLabel "channels" ;
         
         rdfs:range :Channel ;
         
         rdfs:domain :Touchpoint ;
         
         rdfs:subPropertyOf :touchpointObjectProperty .



###  http://ldp.e.corpintra.net/ontology/bce#customerJourneySequence

:customerJourneySequence rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "customer journey sequence" ;
                         
                         rdfs:comment """specialization for customer journeys
This is milieu specific!""" ;
                         
                         rdfs:isDefinedBy "http://ldp.e.corpintra.net/ontology/bce#" ;
                         
                         rdfs:domain :CustomerJourney ;
                         
                         rdfs:range :CustomerJourneySequence ;
                         
                         rdfs:subPropertyOf :pathSequence ;
                         
                         owl:propertyDisjointWith :useCaseSequence .




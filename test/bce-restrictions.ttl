@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfg: <http://www.w3.org/2004/03/trix/rdfg-1/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix fresnel: <http://www.w3.org/2004/09/fresnel#>.
@prefix ordf: <http://purl.org/NET/ordf/>.
@prefix lens: <http://daimler.com/whatever/test/bce#>.
@prefix google: <http://rdf.data-vocabulary.org/#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bce: <http://ldp.e.corpintra.net/ontology/bce#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix oplowl: <http://www.openlinksw.com/ontology/oplowl#> .
@prefix olo: <http://purl.org/ontology/olo/core#> .

bce:Touchpoint a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty skos:prefLabel , bce:channel , bce:duration , bce:prio ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

bce:UseCase a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty skos:prefLabel , bce:tooltip , skos:definition ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] , [
    a owl:Restriction , oplowl:AggregateRestriction ;
    owl:onProperty bce:prio
  ] , [
    a owl:Restriction , oplowl:AggregateRestriction ;
    owl:onProperty bce:useCaseSequence ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

bce:MilieuSpecific a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty bce:milieu ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

bce:Phase a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty skos:prefLabel , skos:definition, bce:tooltip , bce:phaseGroup ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] , [
    a owl:Restriction , oplowl:AggregateRestriction ;
    owl:onProperty bce:phaseOrder ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

olo:OrderedList a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction , oplowl:AggregateRestriction ;
    owl:onProperty olo:slot
  ] .

skos:note a rdf:Property ;
  rdfs:range rdfs:Literal .

# In BCE we use olo:OrderedLists in a specific way which allows to use rdf:Resource as range for olo:item
olo:item a rdf:Property ;
  rdfs:range rdf:Resource .

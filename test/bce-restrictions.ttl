@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfg: <http://www.w3.org/2004/03/trix/rdfg-1/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix fresnel: <http://www.w3.org/2004/09/fresnel#>.
@prefix ordf: <http://purl.org/NET/ordf/>.
@prefix lens: <http://daimler.com/whatever/test/bce#>.
@prefix google: <http://rdf.data-vocabulary.org/#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bce: <http://ldp.e.corpintra.net/ontology/bce#> .
 @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bce:TouchPoint a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty skos:prefLabel , bce:channel , bce:duration , bce:prio ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

bce:UseCase a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty skos:prefLabel , bce:useCaseSequence , bce:tooltip , bce:prio ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

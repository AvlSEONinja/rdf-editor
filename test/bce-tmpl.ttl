@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfg: <http://www.w3.org/2004/03/trix/rdfg-1/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix fresnel: <http://www.w3.org/2004/09/fresnel#>.
@prefix ordf: <http://purl.org/NET/ordf/>.
@prefix lens: <http://daimler.com/whatever/test/bce#>.
@prefix google: <http://rdf.data-vocabulary.org/#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bce: <http://ldp.e.corpintra.net/ontology/bce#> .
@prefix bceui: <http://ldp.e.corpintra.net/ontology/bceui#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix olo: <http://purl.org/ontology/olo/core#> .

lens:TouchPointLens a fresnel:Lens;
    fresnel:classLensDomain bce:Touchpoint;
    fresnel:purpose fresnel:defaultLens;
    fresnel:showProperties (
      skos:prefLabel
      bce:channel
      bce:duration
      bce:financing
      bce:prio
      bce:mediaControl
      bce:reach
      bce:legacyId
      fresnel:allProperties
    ) ;
    fresnel:hideProperties (
      rdf:type
    ) .

lens:UseCaseLens a fresnel:Lens;
    fresnel:classLensDomain bce:UseCase;
    fresnel:purpose fresnel:defaultLens;
    fresnel:showProperties (
      skos:prefLabel
      skos:definition
      bceui:tooltip
      fresnel:allProperties
      bce:prio
      bce:useCaseSequence
    ) ;
    fresnel:hideProperties (
      rdf:type
    ) .

lens:PhaseLens a fresnel:Lens ;
    fresnel:classLensDomain bce:Phase;
    fresnel:purpose fresnel:defaultLens;
    fresnel:showProperties (
      skos:prefLabel
      skos:definition
      bce:legacyId
      bceui:tooltip
      bce:phaseGroup
      bceui:phaseOrder
    ) .

lens:OrderedListLens a fresnel:Lens ;
    fresnel:classLensDomain olo:OrderedList;
    fresnel:purpose fresnel:defaultLens;
    fresnel:showProperties (
      olo:slot
      olo:length
    ) .

lens:OloSlotLens a fresnel:Lens ;
    fresnel:classLensDomain olo:Slot;
    fresnel:purpose fresnel:defaultLens;
    fresnel:showProperties (
      olo:index
      olo:item
    ) .

lens:TouchPointLens a fresnel:Lens;
    fresnel:classLensDomain bce:TouchPoint;
    fresnel:purpose fresnel:defaultLens;
    fresnel:showProperties (
      skos:prefLabel
      bce:channel
      bce:duration
      bce:financing
      bce:prio
      bce:mediaControl
